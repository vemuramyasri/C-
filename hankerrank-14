#include <iostream>
#include <iomanip> 
using namespace std;

int main() {
	int T; cin >> T;
	cout << setiosflags(ios::uppercase);
	cout << setw(0xf) << internal;
	while(T--) {
		double A; cin >> A;
		double B; cin >> B;
		double C; cin >> C;
/* LINE 1: Formatting A */
        // hex: hexadecimal, left: left-aligned, showbase: prefix 0x
        // nouppercase: lowercase hex letters, (long long): truncate decimal
        cout << hex << left << showbase << nouppercase << (long long)A << endl;

        /* LINE 2: Formatting B */
        // dec: reset to decimal, right: right-aligned, setw(15): total width
        // setfill('_'): underscore padding, showpos: force '+' sign
        // fixed: decimal notation, setprecision(2): 2 decimal places
        cout << dec << right << setw(15) << setfill('_') << showpos << fixed << setprecision(2) << B << endl;

        /* LINE 3: Formatting C */
        // scientific: scientific notation, uppercase: use 'E' instead of 'e'
        // noshowpos: remove the '+' sign from the previous step
        // setprecision(9): 9 decimal places
        cout << scientific << uppercase << noshowpos << setprecision(9) << C << endl;
		/* Enter your code here */

	}
	return 0;

}
