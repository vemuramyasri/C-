#include <iostream>
using namespace std;
class Box {
private:
    int length;
public:
    Box(int l) {
        length = l;
    }
    // Declare friend function
    friend void printLength(Box b);
};

// Friend function definition
void printLength(Box b) {
    cout << "Length of box = " << b.length << endl;
}
int main() {
    Box b1(15);
    printLength(b1); // accessing private data using friend function
    return 0;
}
