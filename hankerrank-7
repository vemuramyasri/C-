#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    // Declare the array with the size provided by the user
    int arr[n];

    // Read N integers into the array
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    // Print the array in reverse order
    // We start at index n-1 and decrement (i--) until we reach 0
    for (int i = n - 1; i >= 0; i--) {
        cout << arr[i] << " ";
    }

    return 0;
}
