#include <iostream>
using namespace std;
// Function to modify value
void modify (int  x) {
    x = x + 5 ;   // change applied to the copy
}
int main() {
    int a = 10;  
    cout << "Value of a before function call: " << a << endl ;
    modify(a) ;   // Call by Value
    cout << "Value of a after function call: " << a << endl;  
    return 0;
}
