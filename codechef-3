#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;

    while (T--) {
        int N, X;
        cin >> N >> X;

        int ages[N];
        for (int i = 0; i < N; i++) {
            cin >> ages[i];
        }

        int eligibleVoters = 0;
        for (int i = 0; i < N; i++) {
            if (ages[i] >= X) {
                eligibleVoters++;
            }
        }

        cout << eligibleVoters << endl;
    }

    return 0;
}
